#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n,i,temp=0,k;
    scanf("%d",&n);
    int arr[n];
    scanf("%d",&k);
    for(i=0;i<n;i++)
        scanf("%d",&arr[i]);
    for(i=0;i<n;i++)
    {
        if(arr[i]==k)
            temp=i;
    }
    printf("%d",temp);
    return 0;
}
