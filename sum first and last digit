#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int num,a,s=0;
    scanf("%d",&num);
    a=num%10;
    while(num>=10)
    {
        num=num/10;
    }
    s=a+num;
    printf("%d",s);
    return 0;
}
