#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int n,r,i=1,val=0;
    scanf("%d",&n);
    while(n>0)
    {
       r=n%10;       
          if(r==0)
            val=5*i+val;
        else
            val=r*i+val; 
         i*=10;
       n=n/10;
    }
      printf("%d",val); 
    return 0;
}
