#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    char a[100];
    int i,sum=0;
    scanf("%[^\n]s",a);
    while(a[i]!='\0')
    {
        if(a[i]=='a'&&a[i+1]=='e'&& a[i+2]=='i'&& a[i+3]=='o'&& a[i+4]=='u')
        { sum++;
        }
        i++;
      }
    if(sum==1)
        printf("1");
    else
        printf("0");
    return 0;
}
